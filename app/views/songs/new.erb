<h1>New Song</h1>
<form class="" action="/songs" method="post">
  <p>Name:</p><input type="text" name="song[name]" value="">
  <p>Genres:</p>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>">
  <% end %>
  <p>Artist:</p>
  <input type="text" name="song[artist]" value="">
  <input type="submit" name="" value="Create">
</form>
